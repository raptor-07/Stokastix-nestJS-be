{
  "type": "postgres",
  "host": "localhost",
  "port": "5433",
  "username": "${USER_ROLE}",
  "password": "${USER_PASSWORD}",
  "database": "${DATABASE_NAME}",
  "synchronize": true,
  "logging": true,
  "entities": ["src/entity/**/*.ts"],
  "migrations": ["src/migration/**/*.ts"],
  "subscribers": ["src/subscriber/**/*.ts"]
}
